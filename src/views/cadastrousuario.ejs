<!doctype html>
<html>

<head>
    <meta charset="utf-8">
    <title>Rumos Soluções em T.I. - Orçamentos</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- View Port -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- CSS -->
    <link href="/styles/index.css" rel="stylesheet" type="text/css">
    <link href="/styles/cliente.css" rel="stylesheet" type="text/css">
    <!-- FONTS -->
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans&family=Poppins:wght@500;700&display=swap" rel="stylesheet">

</head>

<body>
    <header>
        <div>
            <img src="/img/rumos logo50-100 PRETO.jpg" alt="Rumos Soluções em TI">
        </div>
    </header>
    <div class="menu">

        <div>
            <a href="/" class="btna"><img src="/img/home.svg" alt="Inicio">
                <p> Início</p>
            </a>
            <a href="/cadastrocliente" class="btna"><img src="/img/clientes.png" alt="Clientes">
                <p>Cadastro de clientes</p>
            </a>
            <a href="/orcamento" class="btna"><img src="/img/orcamento.svg" alt="Orçamento">
                <p>Orçamento</p>
            </a>
            <a href="/cadastrousuario" class="btna"><img src="/img/usuario.png" alt="Usuário">
                <p>Cadastro de Usuário</p>
            </a>
            <a href="/servicos" class="btna"><img src="/img/servicos.svg" alt="Serviços">
                <p>Serviços</p>
            </a>
        </div>

    </div>
    <form action="#">
        <div class="cliente">
            <h2>Cadastro de Usuários</h2>
            <div class="content">
                <p class="cliente">Nome:</p><input type="text"></div>
            <div class="content">
                <p class="cliente">Telefone:</p><input type="text"></div>
            <div class="content">
                <p class="cliente">Sexo:</p><input type="text"></div>
            <div class="content">
                <p class="cliente">CPF:</p><input type="text"></div>
            <div class="content">
                <p class="cliente">Nascimento:</p>:<input type="Date"></div>
            <div class="content">
                <p class="cliente">Cidade:</p><input type="text"></div>
            <div class="content">
                <p class="cliente">Estado:</p><input type="text"></div>
            <div class="content">
                <p class="cliente">Endereço:</p><input type="text"></div>
            <div class="content">
                <p class="cliente">CEP:</p><input type="text"></div>
            <div class="content">
                <p class="cliente">Senha:</p><input type="password"></div>
            <div class="content">
                <p class="cliente">Confirmar Senha:</p><input type="password"></div>
        </div>
    </form>
    <form>
        <button class="btn">Gravar</button>
        <button class="btn">Excluir</button>
        <button class="btn">Novo</button>
        <button class="btn">Consultar Cadastros</button>
    </form>
</body>

</html>